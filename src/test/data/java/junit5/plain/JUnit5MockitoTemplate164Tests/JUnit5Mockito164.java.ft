##----------------------------------------------------------------------------------------------------------------------
## renderAltFlowTestsForDeclaredExceptions $method, $exceptions)
## Renders alt-flow tests where the provided $method is expected to throw the provided $exceptions.
## Param: $method - the method for which to render alt-flow tests.
##----------------------------------------------------------------------------------------------------------------------
#macro(renderAltFlowTestsForDeclaredExceptions $method, $exceptions)
    #foreach($declaredException in $exceptions)
        #renderTestMethod($method, $AltInfo.fromExpectedException($declaredException))
    #end
#end

##----------------------------------------------------------------------------------------------------------------------
## renderTestMethod($method, $altInfo)
## Renders the test for the given Method.
## Param: $method  - the method for which to render the test.
## Param: $altInfo - information about which alternate flow this test should cover.
##----------------------------------------------------------------------------------------------------------------------
#macro(renderTestMethod $method, $altInfo)
#set($_ = $CodeStyleUtils.beginMethodScope())
#if($sourceClass.enum)
    #renderTestMethodForEnum($method, $altInfo)
#elseif($sourceClass.packageVisibleStaticCreatorMethods.contains($method) && !$sourceClass.singleton && !$altInfo.expectedException && !$altInfo.paramWithEmptyIo && !$altInfo.paramWithBrokenIo)
    #renderStaticCreatorMethodPrimaryFlowTest($method)
#else
    ## Determine if the expected return value is empty or absent.
    #set($macro.shouldUseEmptyExpectedValue = $MutableBoolean.create(false))
    #determineExpectedValueEmpty($method, $altInfo, $macro.shouldUseEmptyExpectedValue)
    #set($macro.shouldUseAbsentExpectedValue = $MutableBoolean.create(false))
    #determineExpectedValueAbsent($method, $altInfo, $macro.shouldUseAbsentExpectedValue)
    ## Determine if we should create an assertEquals(...) call.
    #set($macro.returnTypeIsArrayWithComparableType = $method.returnType.array && ($method.returnType.deepArrayComponentType.primitive || $method.returnType.deepArrayComponentType.overridesEquals))
    #set($macro.shouldCreateAssertEqualsCall =
        $method.returnType && ($method.returnType.primitive || $method.returnType.overridesEquals || $method.returnType.array || ($macro.shouldUseAbsentExpectedValue && $method.inferredNullable))
        && ($method.returnType.classT || $method.returnType.allNestedTypeParamsOverrideEquals || $macro.shouldUseEmptyExpectedValue || $macro.shouldUseAbsentExpectedValue || $macro.returnTypeIsArrayWithComparableType)
        && !$altInfo.expectedException)
    #set($macro.shouldStoreExpectedValue = $macro.shouldCreateAssertEqualsCall && !$method.returnType.simple && !$macro.shouldUseEmptyExpectedValue && !$macro.shouldUseAbsentExpectedValue)
    ## Determine the test case suffix to use for this test method and store the result in: $testCaseSuffix.
    #determineTestCaseSuffix($method, $altInfo)

    @Test
    void test${StringUtils.capitalize(${method.name})}${method.overloadSuffix}${testCaseSuffix}() throws Exception {
    // Setup
    ## Declare parameters to use to invoke the method.
    #foreach($param in $method.parameters.filter('shouldStoreInReference', true))
        #if($param == $altInfo.paramWithEmptyIo)
            #declareAndInitLocalVarImpl($param, $param.type.emptyIoInitExpression, true)
        #elseif($param == $altInfo.paramWithBrokenIo)
            #declareAndInitLocalVarImpl($param, $param.type.brokenIoInitExpression, true)
        #else
            #declareAndInitLocalVar($param, true)
        #end
    #end
    #if($macro.shouldStoreExpectedValue)
        #declareAndInitLocalVar($method.returnType, true)
    #end
    #renderStubs($method, $altInfo)

    // Run the test
    #set($macro.qualifier = "#if($method.static)${sourceClass.canonicalNameOrName}#else${sourceClass.testClassMemberName}#end")
    #if($altInfo.expectedException)
        assertThrows(${altInfo.expectedException.type.canonicalNameOrName}.class, () -> ${macro.qualifier}.#methodCall($method, false));
    #else
        #if($method.returnType) final $method.returnType.canonicalText result = #end ${macro.qualifier}.#methodCall($method, false);

        // Verify the results
        #if($macro.shouldCreateAssertEqualsCall)
            #if($macro.shouldUseAbsentExpectedValue && $method.inferredNullable)
                assertNull(result);
            #else
                #set($macro.expectedValueExpression = "#if($macro.shouldStoreExpectedValue)$method.returnType.testClassLocalFieldName #elseif($macro.shouldUseEmptyExpectedValue)$method.returnType.emptyInitExpression #elseif($macro.shouldUseAbsentExpectedValue)$method.returnType.absentInitExpression #else $method.returnType.initExpression #end")
                #assertEqualsCall($macro.expectedValueExpression, 'result', $method.returnType);
            #end
        #end
    #end
    #renderVerifyStatements($method, $altInfo)
    }
#end
#set($_ = $CodeStyleUtils.endMethodScope())
#end

##----------------------------------------------------------------------------------------------------------------------
## renderStaticCreatorMethodPrimaryFlowTest($method)
## Renders the primary flow test case for the given static method that returns an instance of the SourceClass.
## This test case calls methods on the returned source class to ensure it was created properly by the static method.
## Param: $method - the method for which to render the test.
##----------------------------------------------------------------------------------------------------------------------
#macro(renderStaticCreatorMethodPrimaryFlowTest $method)
    #set($macro.shouldRenderSetup = $method.parameters.containsAnyWith('shouldStoreInReference', true))
    @Test
    void test${StringUtils.capitalize(${method.name})}${method.overloadSuffix}() throws Exception {
    #if($macro.shouldRenderSetup)
        // Setup
        ## Declare parameters to use to invoke the method.
        #foreach($param in $method.parameters.filter('shouldStoreInReference', true))
            #declareAndInitLocalVar($param, true)
        #end

    #end
    // Run the test
    final $method.returnType.canonicalText $method.returnType.testClassLocalFieldName = ${sourceClass.canonicalNameOrName}.#methodCall($method, false);
    #set($macro.instanceMethodsToCall = $sourceClass.allNonObjectLowestOverrideMethods.filterOut('static').filter('accessLevel', 'public', 'packagePrivate').filterOut('returnType', $Null).filterOut('simpleSetter').filterOutItemsWithRegex('name', '^(set|with|update)[A-Z0-9_]+.*'))
    #foreach($instanceMethod in $macro.instanceMethodsToCall)
        #foreach($param in $instanceMethod.parameters.filter('shouldStoreInReference', true))
            #declareAndInitLocalVar($param, true)
        #end
        ## There is a bug in the Velocity engine where macro calls in set statements don't work correctly when the
        ## VM_PERM_INLINE_LOCAL setting is enabled. To work around this, define a VTL block containing the macro call, then
        ## evaluate the block and assign the result in a set statement.
        #define($__workaroundCallExpression) ${method.returnType.testClassLocalFieldName}.#methodCall($instanceMethod, false) #end
        #set($macro.callExpression = "$__workaroundCallExpression")
        #assertEqualsCall($instanceMethod.returnType.initExpression, $macro.callExpression, $instanceMethod.returnType);
    #end
    }
#end

##----------------------------------------------------------------------------------------------------------------------
## assertEqualsCall($expected, $actual, $type)
## Renders the assertEquals method call appropriate for the given parameters.
## Param: $expected - the value to use for the expected argument of the assertEquals method call.
## Param: $actual - the value to use for the actual argument of the assertEquals method call.
## Param: $type - the Type of the arguments; this is used to determine which assertEquals method to call.
##----------------------------------------------------------------------------------------------------------------------
#macro(assertEqualsCall $expected, $actual, $type)
    #if($type.array) assertArrayEquals($expected, $actual) #break ## break is needed to avoid adding a newline.
    #elseif($StringUtils.equalsAny($type.canonicalName, 'double', 'java.lang.Double')) assertEquals($expected, $actual, 0.0001) #break
    #elseif($StringUtils.equalsAny($type.canonicalName, 'float', 'java.lang.Float')) assertEquals($expected, $actual, 0.0001) #break
    #elseif($StringUtils.equalsAny($type.canonicalName, 'boolean', 'java.lang.Boolean')) assertTrue($actual) #break
    #else assertEquals($expected, $actual) #end
#end

##----------------------------------------------------------------------------------------------------------------------
## renderStubs($method, $altInfo)
## Renders the stubs (when, doAnswer, doThrow, etc) for the given test case.
## Param: $method - the method for which the test is being rendered.
## Param: $altInfo - information about which alternate flow this test should cover.
##----------------------------------------------------------------------------------------------------------------------
#macro(renderStubs $method, $altInfo)
    #if(!$generateStubsAndVerifyStatements)#break #end
    #set($macro.mockedDIs = $method.dependencyInteractions.satisfiedBy($dependencies.filter('shouldBeMocked')))
    #if($altInfo.diToReturnAbsent)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsBefore($altInfo.diToReturnAbsent))
        #renderAbsentStub($altInfo.diToReturnAbsent)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsAfter($altInfo.diToReturnAbsent))
    #elseif($altInfo.diToReturnEmpty)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsBefore($altInfo.diToReturnEmpty))
        #renderEmptyStub($altInfo.diToReturnEmpty)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsAfter($altInfo.diToReturnEmpty))
    #elseif($altInfo.diToReturnEmptyIo)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsBefore($altInfo.diToReturnEmptyIo))
        #renderEmptyIoStub($altInfo.diToReturnEmptyIo)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsAfter($altInfo.diToReturnEmptyIo))
    #elseif($altInfo.diToReturnBrokenIo)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsBefore($altInfo.diToReturnBrokenIo))
        #renderBrokenIoStub($altInfo.diToReturnBrokenIo)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsAfter($altInfo.diToReturnBrokenIo))
    #elseif($altInfo.diThatThrows)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsBefore($altInfo.diThatThrows))
        #renderExceptionStub($altInfo.diThatThrows, $altInfo.diExceptionToThrow)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsAfter($altInfo.diThatThrows).filterOutItemsWithSameSourceVar($altInfo.diThatThrows))
    #elseif($altInfo.diToReturnFailure)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsBefore($altInfo.diToReturnFailure))
        #renderFailureStub($altInfo.diToReturnFailure)
        #renderPrimaryFlowStubs($macro.mockedDIs.itemsAfter($altInfo.diToReturnFailure).filterOutItemsWithSameSourceVar($altInfo.diToReturnFailure))
    #else
        #renderPrimaryFlowStubs($macro.mockedDIs)
    #end
#end

##----------------------------------------------------------------------------------------------------------------------
## renderPrimaryFlowStubs($dis)
## Renders the primary flow stubs (when, doAnswer, etc) for the given dependency interactions.
## Param: $dis - the dependency interactions to render stubs for.
##----------------------------------------------------------------------------------------------------------------------
#macro(renderPrimaryFlowStubs $dis)
    #foreach($di in $dis)
        #renderPrimaryFlowStub($di)
    #end
#end

##----------------------------------------------------------------------------------------------------------------------
## renderPrimaryFlowStubs($di)
## Renders the primary flow stub (when, doAnswer, etc) for the given dependency interaction.
## Param: $di - the dependency interaction to render stubs for.
##----------------------------------------------------------------------------------------------------------------------
#macro(renderPrimaryFlowStub $di)
    #set($macro.sourceVar = $di.field.possibleSourceVariables.intersection($dependencies).first)
    #set($macro.qualifier = "#if($shouldSetPackageLocalFields)${sourceClass.testClassMemberName}.${macro.sourceVar.declaredName}#else $macro.sourceVar.testClassMemberName #end")
    #if($di.method.parameters.containsAnyWith('type.canonicalName', 'java.lang.Runnable'))
        #renderAnswerForRunnable($di)
    #elseif($di.method.parameters.containsAnyWith('type.canonicalName', 'java.util.concurrent.Callable'))
        #renderAnswerForCallable($di)
    #elseif($di.method.returnType)
        #set($macro.hasInnerWildcardType = $di.method.returnType.parameters.containsAnyWith('wildcard', true))
        ## Declare a local variable for the return type if needed.
        #if($di.method.returnType.shouldStoreInReference)

            // Configure ${di.field.type.name}.${di.method.name}(...).
            #declareAndInitLocalVar($di.method.returnType, false)
        #end
