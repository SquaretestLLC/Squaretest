        #end
    #end
    #if(!$macro.sourceMethodAndDIReturnSameType)
        #set($isAbsentHolder.value = false)
        #break($macro)
    #end

    ## Determine if other DIs return the same type as the source method.
    #if($macro.otherDIs.containsAnyWith('method.returnType.canonicalText', $method.returnType.canonicalText)
        || $macro.otherDIs.containsAnyWith('method.returnType.parameters.first.canonicalText', $method.returnType.canonicalText))
